</style><div role="complementary" id="covid19-callout-box"><div class="covid19-btn-opener">Coronavirus Updates <span id="covid-carot" style="margin-left: 5px;font-size: 1.8rem;"><i class="fas fa-caret-up" aria-hidden="true"></i></span></div><div class="covid19-btn-body"><h3>Coronavirus: Chapman Community  Updates and Resources</h3><a id="covid19-learn-more-btn" class="btn btn-primary btn-block" aria-label="Learn more about Corona Virus" href="https://news.chapman.edu/coronavirus/">Visit Website</a></div></div><script>
var contactEl;
var toggleBtn;
var contactElHidden = true;

$(function() {
    contactEl = $('#covid19-callout-box');
    toggleBtn = contactEl.find('.covid19-btn-opener');
    toggleBtn.click(_toggleContactEl);
    $(window).scroll(_onScroll);
});

function _toggleContactEl() {
    TweenLite.to(contactEl, 0.3, {
        x: contactElHidden ? '0%' : '100%'
    });
    contactElHidden = !contactElHidden;
    $("#covid-carot i").toggleClass('fa-caret-up fa-caret-down');
}

function _onScroll() {
    // scrolled half-way
    if ($(window).scrollTop() < $(document).height() * 0.5) {
        if (contactElHidden) {
            _toggleContactEl();
        }
        // only try to display once
        $(window).off('scroll');
    }
}
</script>

